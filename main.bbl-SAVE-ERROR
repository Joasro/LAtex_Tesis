      \strng{namehash}{b0fc8c916022b7c1b256c8cb017c5c54}
      \strng{fullhash}{06261af94a508b28edef7745378eafe2}
      \strng{fullhashraw}{06261af94a508b28edef7745378eafe2}
      \strng{bibnamehash}{06261af94a508b28edef7745378eafe2}
      \strng{authorbibnamehash}{06261af94a508b28edef7745378eafe2}
      \strng{authornamehash}{b0fc8c916022b7c1b256c8cb017c5c54}
