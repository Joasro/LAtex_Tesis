    \entry{barthelemy_spatial_2011}{article}{}{}
      \name{author}{1}{}{%
        {{hash=dccb20a7c90ae422e209daa791149063}{%
           family={Barthelemy},
           familyi={B\bibinitperiod},
           given={Marc},
           giveni={M\bibinitperiod}}}%
      }
      \list{language}{1}{%
        {en}%
      }
      \strng{namehash}{dccb20a7c90ae422e209daa791149063}
      \strng{fullhash}{dccb20a7c90ae422e209daa791149063}
      \strng{fullhashraw}{dccb20a7c90ae422e209daa791149063}
      \strng{bibnamehash}{dccb20a7c90ae422e209daa791149063}
      \strng{authorbibnamehash}{dccb20a7c90ae422e209daa791149063}
