    \entry{liu_estimating_2024}{article}{}{}
      \name{author}{4}{}{%
        {{hash=17a1772c22ce40a67db8f54dafaeb701}{%
           family={Liu},
           familyi={L\bibinitperiod},
           given={Xiaohui},
