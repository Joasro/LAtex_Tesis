      \strng{namehash}{168796e290a6aa51eda217b1d581c199}
      \strng{fullhash}{b96ca18db0d8775587db6ac8883475c8}
      \strng{fullhashraw}{b96ca18db0d8775587db6ac8883475c8}
      \strng{bibnamehash}{168796e290a6aa51eda217b1d581c199}
      \strng{authorbibnamehash}{168796e290a6aa51eda217b1d581c199}
