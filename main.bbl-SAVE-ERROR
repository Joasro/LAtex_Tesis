      \strng{namehash}{5ddeae9368c931e39c21152f937c2ed0}
      \strng{fullhash}{5ddeae9368c931e39c21152f937c2ed0}
      \strng{fullhashraw}{5ddeae9368c931e39c21152f937c2ed0}
      \strng{bibnamehash}{5ddeae9368c931e39c21152f937c2ed0}
      \strng{authorbibnamehash}{5ddeae9368c931e39c21152f937c2ed0}
