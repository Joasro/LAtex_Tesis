@article{shenkoya_sustainability_2023,
	title = {Sustainability in Higher Education: Digital Transformation of the Fourth Industrial Revolution and Its Impact on Open Knowledge},
	volume = {15},
	issn = {2071-1050},
	url = {https://www.mdpi.com/2071-1050/15/3/2473},
	doi = {10.3390/su15032473},
	shorttitle = {Sustainability in Higher Education},
	abstract = {Education is important for sustainable development and drives innovation within national innovation systems. As developed economies move to matured stages of the fourth industrial revolution, the quality of education needs to keep up with the required technological changes brought about by the digital transformation created by these technologies. However, studies on the impact of the digital transformation of the higher education system and its effect on sustainability are few. This study bridged this gap by providing empirical evidence on the subject matter through a synergy of extant studies. The methodology used herein involves a combination of a systematic literature review and a bibliometric analysis. The results of this study show that the digital transformation of the higher education sector is leading to the development of sustainable curriculums, digitalization of the higher education, enhancement of innovation, and an improvement in the performance of students. It also shows that the future of the digital transformation of the higher education sector will include—the advancement of the concept of ‘Education 4.0’, an increase in gamification within the sector, a rise in the use of datafication in decision making, and the integration of artificial intelligence/augmented intelligence to reform higher education.},
	pages = {2473},
	number = {3},
	journaltitle = {Sustainability},
	shortjournal = {Sustainability},
	author = {Shenkoya, Temitayo and Kim, Euiseok},
	urldate = {2026-02-07},
	date = {2023-01-30},
	langid = {english},
}

@article{guadalupe_beltran_desafios_2025,
	title = {Desafíos de la Educación Superior en Contextos Híbridos: Análisis de las Prácticas Docentes en la Universidad Estatal de Milagro durante el Periodo Académico 2025},
	volume = {6},
	rights = {https://creativecommons.org/licenses/by/4.0},
	issn = {2965-6052, 2595-2021},
	url = {https://revistaveritas.org/index.php/veritas/article/view/685},
	doi = {10.61616/rvdc.v6i2.685},
	shorttitle = {Desafíos de la Educación Superior en Contextos Híbridos},
	abstract = {El presente artículo tiene como objetivo analizar los principales desafíos que enfrenta la educación superior en contextos híbridos, centrándose en las prácticas docentes desarrolladas en la Universidad Estatal de Milagro durante el periodo académico 2025. Para alcanzar este propósito, se implementó un enfoque metodológico mixto, que combinó la aplicación de encuestas a docentes y estudiantes, entrevistas semiestructuradas y la observación participante en clases híbridas. Los resultados evidencian que, si bien la adopción de modelos híbridos ha permitido una mayor flexibilidad y acceso a la educación, persisten retos significativos en cuanto a la adaptación pedagógica, el manejo de herramientas tecnológicas y la evaluación del aprendizaje. Asimismo, se identificó la necesidad de fortalecer la formación docente continua y el diseño de estrategias didácticas innovadoras que respondan a la diversidad de los estudiantes. El estudio concluye que la consolidación de la educación híbrida requiere una visión institucional integral, así como el compromiso de los actores educativos para superar las barreras identificadas y potenciar las oportunidades que ofrece este modelo. Además, se observó que la interacción entre estudiantes y docentes en entornos híbridos demanda nuevas competencias comunicativas y una gestión más efectiva del tiempo y los recursos digitales. La investigación subraya la importancia de fomentar espacios de colaboración y retroalimentación constante que permitan ajustar las prácticas pedagógicas en función de las necesidades emergentes.},
	pages = {1259--1281},
	number = {2},
	journaltitle = {Revista Veritas de Difusão Científica},
	shortjournal = {Veritas},
	author = {Guadalupe Beltrán, Eduardo Stalyn and Palomeque Zambrano, Jeniffer Yadira and Loor Avila, Beatriz Annabell},
	urldate = {2026-02-07},
	date = {2025-07-15},
}

