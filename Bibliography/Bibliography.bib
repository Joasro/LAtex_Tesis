@article{pillajo_pila_impacto_2025,
	title = {El Impacto del Aprendizaje Híbrido en Estudiantes Universitarios en el Siglo {XXI}: Efectividad, Beneficios y Desafíos en América Latina: The Impact of Hybrid Learning on University Students in the 21st Century: Effectiveness, Benefits, and Challenges in Latin America},
