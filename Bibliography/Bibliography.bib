	file = {Versión enviada:C\:\\Users\\Joasro\\Zotero\\storage\\JFNX2569\\Sweet et al. - 2023 - Cross-Validation Strategy Impacts the Performance and Interpretation of Machine Learning Models.pdf:application/pdf},
}

