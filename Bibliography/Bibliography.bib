	langid = {english},
	file = {Texto completo:C\:\\Users\\Joasro\\Zotero\\storage\\YTB93DIP\\Himeur et al. - 2023 - AI-big data analytics for building automation and management systems a survey, actual challenges an.pdf:application/pdf},
}


@article{kustitskaya_designing_2023,
