@article{nellore_traffic_2016,
	title = {Traffic {Management} for {Emergency} {Vehicle} {Priority} {Based} on {Visual} {Sensing}},
	volume = {16},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
