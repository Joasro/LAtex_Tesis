@article{alvarez_percepcion_2025,
	title = {Percepción de la congestión del tráfico vehicular y su influencia en el rendimiento y bienestar de los estudiantes universitarios de una universidad privada en la ciudad de {Asunción}, {Paraguay}, 2024},
	volume = {12},
	copyright = {Derechos de autor 2025},
	issn = {2409-8752},
	url = {https://revista.ucsa-ct.edu.py/ojs/index.php/ucsa/article/view/378},
	doi = {10.18004/ucsa/2409-8752/2025.012.01.054},
	abstract = {Vehicular traffic congestion is a growing problem in Paraguay, with negative effects on society and people's quality of life. This research focuses on analyzing the perception of students at a private university on vehicular traffic congestion and its influence on academic performance and well-being in the year 2024. Regarding the methodology, a mixed approach complemented by a descriptive non-experimental and cross-sectional scope has been chosen. A convenience sample of sixty-three students was selected, to whom an anonymous survey was administered as a data collection instrument. The results indicate that students are highly exposed to vehicular traffic congestion due to daily attendance at the university and the use of automobiles and public transportation. On the other hand, it was identified that vehicular traffic congestion moderately affects students' academic performance. Finally, based on student perception, it is concluded that greater investment in road infrastructure and improvement of the public transportation system are key factors for improving urban travel, not only in the city of Asunción, but also in other cities in the country.},
	language = {es},
	number = {1},
	urldate = {2025-10-07},
